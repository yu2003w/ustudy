LOAD DATA LOCAL INFILE '/root/mysql/schema/sample/school.csv' INTO TABLE ustudy.school
  CHARACTER SET UTF8
  FIELDS TERMINATED BY ',' ENCLOSED BY '"'
  LINES TERMINATED BY '\n'
  IGNORE 1 LINES;
