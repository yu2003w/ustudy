LOAD DATA LOCAL INFILE '/root/mysql/schema/sample/users.csv' REPLACE INTO TABLE ustudy.users
  CHARACTER SET UTF8
  FIELDS TERMINATED BY ',' ENCLOSED BY '"'
  LINES TERMINATED BY '\n'
  IGNORE 1 LINES;
